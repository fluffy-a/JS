let number1=parseInt(prompt("Enter your first number:"));
let divider=parseInt(prompt("Diveded by:?"));
function DividesTwoNumber(number1,divider){
    let divide=number1 % divider;
    console.log(divide);
    return divide;

}

DividesTwoNumber(number1,divider);

Homework 5 exercise 

function Animals(chickens,cows,pigs){
    let chickensLegs=chickens*2;
    let cowsLegs=cows*4;
    let pigsLegs=pigs*4;
    return(chickensLegs+cowsLegs+pigsLegs);
// alert(`The total number of legs is${totalLegs}. `)
}
let Animals1=Animals(5,2,2);
console.log(Animals1);


Homework 6 exercise

let firstString=prompt("Insert the 1st string:");
let secondString=prompt("Insert the 2nd string:");

function Comparation(firstString,secondString){
    if (firstString.length===secondString.length){
        let IsTrue=true;
        console.log(isTrue);
        return isTrue;
    }
    else {
        let isFalse=false;
        console.log(isFalse)
        return isFalse;
    }
}
Comparation(firstString,secondString);


also answer
function stringsLength(firstString,secondString) {
    let string1 = firstString.length;
    let string2 = secondString.length;
    return string1 === string2;
}
let stringsLength1 = stringsLength("Just", "Saying");
console.log(stringsLength1);


homework exercise 7
function Plural(first){
    return first.endsWith('s')
}
let Plural1=Plural('Crowns');
console.log(Plural1);

homework ex 8

function stringsLowerCase(first,second){
    let first1=first;
    let second2=second;
    return first1.toLowerCase(first)===second2.toLowerCase(second);
}
let stringsLowerCase1=stringsLowerCase("heLlO","hello");
console.log(stringsLowerCase1);


homework ex 9
let month=5;
switch(month){
    case 1:
        console.log("January")
        break;
    case 2:
        console.log("February")
        break;
    case 3:
        console.log("March")
        break;
    case 4:
        console.log("April")
        break;
    case 5:
        console.log("May")
        break;
    case 6:
        console.log("June")
        break;
    case 7:
        console.log("July")
        break;
    case 8:
        console.log("August")
        break;
    case 9:
        console.log("September")
        break;
    case 10:
        console.log("October")
        break;
    case 11:
        console.log("November")
        break;
    case 12:
        console.log("December")
        break;
    default:
        console.log("Month with that number does not exists")
        break;
}
    console.log("finished");

homework 10
function isTruthy(a) {
    if (a == false) {
        console.log(0);
    } else if (a == null) {
        console.log(0);
    } else if (a == undefined) {
        console.log(0);
    } else if (a == 0) {
        console.log(0);
    } else if (a == NaN) {
        console.log(0);
    } else if (a == "") {
        console.log(0);
    } else {
        console.log(1);
    }
    return typeof a;
}
let isTruthy1 = isTruthy(false);
console.log(isTruthy1);



function Winner(teamA,scoreA,teamB,scoreB){
    let score= `${scoreA}: ${scoreB}`;
    if ((typeof(teamA) === "string") 
    && (typeof(teamB) === "string")
    &&(typeof(scoreA)==="number" && scoreA>=0)
    &&(typeof(scoreB)==="number" && scoreB>=0))

    if(scoreA>scoreB){
        return(` ${teamA} has beaten teamB  by ${scoreA} `);
    }
    else if(scoreB<ScoreA){
        return( `teamB has beaten teamA by ${scoreB} `);
    }
    else {
        return("The match was a draw with a score of SCORE:${scoreA && scoreB}")
    }
}

console.log("Manchester",10,"Liverpool",15)
