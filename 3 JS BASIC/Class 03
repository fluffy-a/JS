also second answer
function SumOfNumbers(num1){
    let nextNumber=+1;
    let result=num1+nextNumber;
    return result;
}
    console.log(SumOfNumbers (20));

Homework 2 exercise
function conversionMinutesToSeconds(){
   let seconds=60;
   let  minutes=10;
   let result=seconds*minutes;
   console.log(result);
}
conversionMinutesToSeconds();
 
also answer

let minutes=parseInt(prompt("How many minutes?"));
function howManySeconds(minutes){
    let seconds=minutes*60;
    console.log(`In ${minutes}minutes there are ${seconds}seconds. `)
}
howManySeconds(minutes);

Homework 3 exercise
let hours=parseInt(prompt("How many hours?"));
function howManySeconds(hours){
    let seconds=hours*3600;
    console.log(`In ${hours}hours there are ${seconds}seconds. `)
}
howManySeconds(hours);

